<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:context="http://www.springframework.org/schema/context"
  xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context 
    http://www.springframework.org/schema/context/spring-context.xsd"
> 
	<context:property-placeholder location="XmlAnnotJBaseConfExample/values.properties"/>
	<context:annotation-config></context:annotation-config>
	<bean id="manager" class="XmlAnnotJBaseConfExample.DatabaseManager">
	</bean>
	<bean id="absdatabase" abstract="true">
		<property name="name" value="${name}"></property>
		<property name="connectWord" value="${connectWord}"></property>
		<property name="ports" >
			<array>
				<value>8080</value>
				<value>8081</value>
			</array>
		</property>
	</bean>
	<bean id="MySql" primary="true" parent="absdatabase" class="XmlAnnotJBaseConfExample.MySqlDatabase">
	<qualifier></qualifier>
	</bean>
	<bean id="Oracle" parent="absdatabase" class="XmlAnnotJBaseConfExample.OracleDatabase">
	</bean>
	

</beans>